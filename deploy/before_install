sudo rm -rf /var/www/html/bluebonnet/resources
sudo rm -rf /var/www/html/bluebonnet/routes
sudo rm -rf /var/www/html/bluebonnet/tests
sudo rm -rf /var/www/html/bluebonnet/vendor
sudo rm -f /var/www/html/bluebonnet/.env.example
sudo rm -f /var/www/html/bluebonnet/.gitattributes
sudo rm -f /var/www/html/bluebonnet/.gitignore
sudo rm -f /var/www/html/bluebonnet/.appspec.yml
sudo rm -f /var/www/html/bluebonnet/artisan
sudo rm -f /var/www/html/bluebonnet/composer.json
sudo rm -f /var/www/html/bluebonnet/composer.lock
sudo rm -f /var/www/html/bluebonnet/package-lock.json
sudo rm -f /var/www/html/bluebonnet/package.json
sudo rm -f /var/www/html/bluebonnet/phpunit.xml
sudo rm -f /var/www/html/bluebonnet/server.php
sudo rm -f /var/www/html/bluebonnet/webpack.mix.js
